{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning",
			"Microsoft.EntityFrameworkCore.Database.Command": "Information"
		}
	},
	"AllowedHosts": "*",
	"Serilog": {
		"Using": ["Serilog.Sinks.Console"],
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"Microsoft": "Warning",
				"Microsoft.EntityFrameworkCore.Database.Command": "Information",
				"System": "Warning"
			}
		},
		"WriteTo": [
			{
				"Name": "Console",
				"Args": {
					"formatter": "Serilog.Formatting.Compact.CompactJsonFormatter,Serilog.Formatting.Compact"
				}
			}
		],
		"Enrich": ["FromLogContext", "WithThreadId", "WithMachineName"]
	},
	"ConnectionStrings": {
		"DefaultConnection": "Server=10.0.10.15;Port=5432;Database=dadvice;Username=postgres;Password=password;Include Error Detail=True"
	}
}
